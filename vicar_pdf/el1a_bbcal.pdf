edibis    tab_&id comm=("(11,1) set 11")
edibis    tab_&id comm=("(11,2) set &avg")
edibis    tab_&id comm=("(11,3) set &sd")
!Zero-out RTD 4
f2        img_&id im2_&id func="(samp!=4)*in1"
list      im2_&id
!Combo 12: RTDs 1,2,3,5
hist      im2_&id size=(1,1,1,5) mean=avg sigma=sd 'nohi 'excl
edibis    tab_&id comm=("(12,1) set 12")
edibis    tab_&id comm=("(12,2) set &avg")
edibis    tab_&id comm=("(12,3) set &sd")
!Combo 13: RTDs 2,3,5
hist      im2_&id size=(1,2,1,4) mean=avg sigma=sd 'nohi 'excl
edibis    tab_&id comm=("(13,1) set 13")
