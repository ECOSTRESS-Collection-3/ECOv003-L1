edibis    tab_&id comm=("(14,1) set 14")
edibis    tab_&id comm=("(14,2) set &avg")
edibis    tab_&id comm=("(14,3) set &sd")
!Zero-out RTD 2&4
f2        img_&id im2_&id func="((samp!=2)&(samp!=4))*in1"
list      im2_&id
!Combo 15: RTDs 1,3,5
hist      im2_&id size=(1,1,1,5) mean=avg sigma=sd 'nohi 'excl
edibis    tab_&id comm=("(15,1) set 15")
edibis    tab_&id comm=("(15,2) set &avg")
edibis    tab_&id comm=("(15,3) set &sd")
!Zero-out RTD 2&3
f2        img_&id im2_&id func="((samp!=2)&(samp!=3))*in1"
list      im2_&id
!Combo 16: RTDs 1,4,5
hist      im2_&id size=(1,1,1,5) mean=avg sigma=sd 'nohi 'excl
edibis    tab_&id comm=("(16,1) set 16")
edibis    tab_&id comm=("(16,2) set &avg")
edibis    tab_&id comm=("(16,3) set &sd")
write     "Column1 Table Codes:"
write     "1:  RTDs 1,2,3,4,5"
write     "2:  RTDs 1,2,3,4"
write     "3:  RTDs 1,2,3"
write     "4:  RTDs 2,3,4,5"
write     "5:  RTDs 2,3,4"
write     "6:  RTDs 3,4,5"
write     "7:  RTDs 1,3,4,5"
write     "8:  RTDs 1,3,4"
write     "9:  RTDs 1,2,4,5"
write     "10: RTDs 1,2,4"
write     "11: RTDs 2,4,5"
write     "12: RTDs 1,2,3,5"
write     "13: RTDs 2,3,5"
write     "14: RTDs 1,2,5"
write     "15: RTDs 1,3,5"
write     "16: RTDs 1,4,5"
!Analyze statistics and select the best Blackbody Temperature
sort      tab_&id sortcol=(3,2) 'ascend  !Bring lowest STD to top
ibis-l    tab_&id cols=(1,2,3) cfor="%5.0f %11.3f %11.6f"
rowop2    tab_&id tab2_&id keycol=(2,2) +
          range=(-9999,310,330,9999) 'delete !Remove BAD Temps
!Note: if no combos meet Rowop2 requirements, the Ibis2tcl step will
!error, Temp & STD will default to init (0.0), and the UPF-supplied
!temperature values will be used. 
ibis2tcl  tab2_&id v1=temp v2=std ibisloc=(1,2,1,3) vartype=(0,0)
ibis-l    tab_&id nr=1 cols=(1,2,3) cfor="%5.0f %11.3f %11.6f"
write    "MSG: ***RTD Temp= &temp  RTD STD= &std "
